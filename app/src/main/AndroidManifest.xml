<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coursera.miprimer">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ContactoActivity"
            android:label="@string/title_activity_contacto"
            android:parentActivityName=".Mascotas"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.coursera.miprimer.Mascotas" />
        </activity>
        <activity
            android:name=".AcercaDe"
            android:label="@string/title_activity_acerca_de"
            android:parentActivityName=".Mascotas"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.coursera.miprimer.Mascotas" />
        </activity>
        <activity
            android:name=".MascotasFavoritas"
            android:parentActivityName=".Mascotas" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Mascotas">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>